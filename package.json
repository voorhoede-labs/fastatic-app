{
  "name": "fastatic-ui",
  "version": "0.1.0",
  "description": "A Graphical User Interface for the fastatic node module",
  "main": "main.js",
  "scripts": {
    "prestart": "lessc ui/styles.less ui/styles.css --source-map --source-map-rootpath=ui/ & riot ui/components ui/compiled-tags.js --ext html",
    "start": "NODE_ENV=development nodejs-dashboard electron .",
    "lint": "eslint",
    "prepackage": "rimraf releases && icon-gen --input assets/icon.svg --output ./releases/icons --report",
    "package": "electron-packager . --out=releases --overwrite --no-prune --icon=./releases/icons/app.icns"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/voorhoede-labs/fastatic-gui.git"
  },
  "bugs": {
    "url": "https://github.com/voorhoede-labs/fastatic-gui/issues"
  },
  "homepage": "https://github.com/voorhoede-labs/fastatic-gui#readme",
  "keywords": [
    "fastatic",
    "ui",
    "optimization",
    "static"
  ],
  "author": "De Voorhoede (https://voorhoede.nl/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/voorhoede-labs/fastatic-gui/issues"
  },
  "homepage": "https://github.com/voorhoede-labs/fastatic-gui#readme",
  "dependencies": {
    "about-window": "1.6.0",
    "bluebird": "3.4.6",
    "calc-percent": "1.0.1",
    "electron": "1.3.4",
    "electron-devtools-installer": "2.0.1",
    "filesize": "3.3.0",
    "ncp": "2.0.0",
    "nodejs-dashboard": "0.1.0",
    "redux": "3.6.0",
    "redux-devtools-extension": "1.0.0",
    "rimraf": "2.5.4",
    "riot": "2.6.1"
  },
  "devDependencies": {
    "electron-packager": "8.2.0",
    "eslint": "3.5.0",
    "eslint-config-airbnb": "11.1.0",
    "eslint-plugin-html": "1.5.2",
    "eslint-plugin-import": "1.15.0",
    "eslint-plugin-jsx-a11y": "2.2.2",
    "eslint-plugin-react": "6.2.2",
    "icon-gen": "1.0.6",
    "less": "2.7.1"
  }
}
